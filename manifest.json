{
  "name": "MoodyMind",
  "description": "A mental health journal to record your mood and thoughts.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "template.html",
    "default_icon": "assets/extension-logo.png",
    "scripts": [ "./src/index.js" ]
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "type": "module",
    "minimum_chrome_version": "92"

  }
}
