{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "template.html",
    "default_icon": "hello_extensions.png",
    "scripts": [ "./src/index.js" ]
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "type": "module",
    "minimum_chrome_version": "92"

  }
}
